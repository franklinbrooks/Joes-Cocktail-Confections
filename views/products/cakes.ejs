<% include ../partials/head.ejs %>
  <body>
    <% include ../partials/header.ejs %>
    <main>
      <div class="container">
        <div class="wrapper">
          <h1><i>Cakes</i></h1>
          <% cakes.forEach(function(cake) {%>
          <div>
          <img src="<%=cake.image%>" alt=" <%= cake.productName %> image" height="200" width="200" >
          <h4><%= cake.productName %></h4>
          <p> Priced From $ <%= (cake.price *.01).toFixed(2) %></p>
          <div><a class="waves-light btn-large blue"  href="/products/<%= cake.id %>">View More</a></div>
          <br>
          <hr>
          </div>
       <% }) %>
      </div>
      </div>
    </main>
    <% include ../partials/footer.ejs %>
  </body>
</html>
